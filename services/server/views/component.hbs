<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>Voyager</title>

    <link rel="stylesheet" href="/css/normalize.css">
    <link rel="stylesheet" href="/css/fontawesome.min.css">
    <link rel="stylesheet" href="/css/fontawesome-solid.min.css">
    <link rel="stylesheet" href="/app/css/voyager-{{component}}.css">

    {{#if devMode}}
        {{#if local}}
            <!--<script src="/libs/es6-promise.auto.min.js"></script>-->
            <script src="/libs/react.development.js"></script>
            <script src="/libs/react-dom.development.js"></script>
            <script src="/libs/pep.js"></script>
            <script src="/libs/three.js"></script>
        {{else}}
            <!--<script src="https://cdn.jsdelivr.net/npm/es6-promise@4/dist/es6-promise.auto.min.js"></script>-->
            <script src="https://cdnjs.cloudflare.com/ajax/libs/react/16.6.3/umd/react.development.js"></script>
            <script src="https://cdnjs.cloudflare.com/ajax/libs/react-dom/16.6.3/umd/react-dom.development.js"></script>
            <script src="https://code.jquery.com/pep/0.4.3/pep.js"></script>
            <script src="https://cdnjs.cloudflare.com/ajax/libs/three.js/99/three.js"></script>
        {{/if}}
    {{else}}
        {{#if local}}
            <!--<script src="/libs/es6-promise.auto.min.js"></script>-->
            <script src="/libs/react.production.min.js"></script>
            <script src="/libs/react-dom.production.min.js"></script>
            <script src="/libs/pep.min.js"></script>
            <script src="/libs/three.min.js"></script>
        {{else}}
            <!--<script src="https://cdn.jsdelivr.net/npm/es6-promise@4/dist/es6-promise.auto.min.js"></script>-->
            <script src="https://cdnjs.cloudflare.com/ajax/libs/react/16.6.3/umd/react.production.min.js"></script>
            <script src="https://cdnjs.cloudflare.com/ajax/libs/react-dom/16.6.3/umd/react-dom.production.min.js"></script>
            <script src="https://code.jquery.com/pep/0.4.3/pep.min.js"></script>
            <script src="https://cdnjs.cloudflare.com/ajax/libs/three.js/99/three.min.js"></script>
        {{/if}}
    {{/if}}

</head>
<body>
{{#if devMode}}
    <script src="app/js/voyager-{{component}}.js"></script>
{{else}}
    <script src="app/js/voyager-{{component}}.min.js"></script>
{{/if}}
    <voyager-{{component}}></voyager-{{component}}>
</body>
</html>


