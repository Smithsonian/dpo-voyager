<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">

        <title>{{htmlWebpackPlugin.options.title}}</title>

        <link rel="shortcut icon" type="image/png" href="favicon.png"/>
        <link href="/fonts/fonts.css" rel="stylesheet">

        <style>
            body { background: #303030; }
        </style>

        {{#if htmlWebpackPlugin.options.isDevelopment}}
            {{#if htmlWebpackPlugin.options.isOffline}}
        <script src="/libs/three.js"></script>
        <link href="/libs/quill.snow.css" rel="stylesheet">
        <script src="/libs/quill.js"></script>
            {{else}}
        <link href="https://cdnjs.cloudflare.com/ajax/libs/quill/1.3.7/quill.snow.css" rel="stylesheet">
        <script src="https://cdnjs.cloudflare.com/ajax/libs/quill/1.3.7/quill.js"></script>
            {{/if}}
        {{else}}
            {{#if htmlWebpackPlugin.options.isOffline}}
        <script src="/libs/three.min.js"></script>
        <link href="/libs/quill.snow.css" rel="stylesheet">
        <script src="/libs/quill.min.js"></script>
            {{else}}
        <link href="https://cdnjs.cloudflare.com/ajax/libs/quill/1.3.7/quill.snow.min.css" rel="stylesheet">
        <script src="https://cdnjs.cloudflare.com/ajax/libs/quill/1.3.7/quill.min.js"></script>
            {{/if}}
        {{/if}}

    </head>
    <body>
        {{{htmlWebpackPlugin.options.element}}}
    </body>
</html>